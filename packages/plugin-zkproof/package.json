{
  "name": "@elizaos-plugins/plugin-zkproof",
  "version": "1.0.0",
  "type": "module",
  "description": "ZK proof plugin for anonymizing Clerk and Stripe IDs using Noir & Barretenberg/Garaga backends.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "@elizaos/source": "./src/index.ts",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --format esm --dts",
    "dev": "tsup --format esm --dts --watch",
    "compile-circuits": "./scripts/compile-circuits.sh",
    "publish-artifacts": "./scripts/publish-artifacts.sh"
  },
  "dependencies": {
    "@elizaos/core": "workspace:*",
    "@noir-lang/noir_js": "^0.23.0",
    "@aztec/bb.js": "^0.23.0",
    "starknet": "^6.18.0",
    "tsup": "8.3.5"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "@types/node": "^22.10.5"
  },
  "peerDependencies": {
    "whatwg-url": "7.1.0"
  },
  "agentConfig": {
    "pluginType": "elizaos:client:1.0.0",
    "pluginParameters": {
      "ZK_BACKEND": {
        "type": "string",
        "enum": ["barretenberg", "garaga"],
        "description": "Configurable proof backend for the ZK module (default: barretenberg)"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
