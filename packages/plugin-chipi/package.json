{
  "name": "@elizaos-plugins/plugin-chipi",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest run"
  },
  "files": [
    "dist",
    "src",
    "cairo"
  ],
  "dependencies": {
    "@chipi-stack/chipi-react": "^11.2.0",
    "@clerk/clerk-react": "^5.48.1",
    "@elizaos-plugins/plugin-email": "workspace:*",
    "@elizaos-plugins/plugin-shared-email-sanity": "workspace:*",
    "@elizaos/core": "workspace:^",
    "@sanity/client": "^6.20.0",
    "@uniswap/sdk-core": "^5.0.0",
    "node-cache": "^5.1.2",
    "sanity": "^4.6.1",
    "starknet": "^6.18.0",
    "zod": "^3.22.0",
    "tsup": "8.3.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.0.2",
    "typescript": "^5.1.6",
    "vitest": "^2.1.9"
  },
  "license": "UNLICENSED",
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "CHIPI_PUBLIC_API_KEY": {
        "type": "string",
        "optional": false
      },
      "CHIPI_SECRET_KEY": {
        "type": "string",
        "optional": false
      }
    }
  }
}